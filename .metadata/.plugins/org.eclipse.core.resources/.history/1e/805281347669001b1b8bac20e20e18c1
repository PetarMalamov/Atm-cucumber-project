package Models;

public class AtmModel {

	public void creditCardInsertedMessage() {
		System.out.println("Картата е поставена");
	}

	public void printChoosenOption(String optiion) {
		String.format("Избраната опция е : %s", optiion);
	}
	
	public void printListWithAmounts() {
		System.out.println("Choose from amounts 123,321,234,345");
	}
	
	public void printChoosenAmount(String optiion) {
		String.format("Избраната сума е : %s", optiion);
	}
}
